{
    "schemaVersion": 1,
    "id": "oldanimations",
    "version": "${version}",
    "name": "Old Animations",
    "description": "Fabric mod to restore Minecraft animations from legacy versions.",
    "authors": [
        "nekkan"
    ],
    "icon": "assets/oldanimations/logo.png",
    "contact": {
        "homepage": "https://www.curseforge.com/minecraft/mc-mods/old-animations",
        "issues": "https://github.com/nekkan/old-animations/issues",
        "sources": "https://github.com/nekkan/old-animations"
    },
    "license": "MIT",
    "environment": "client",
    "entrypoints": {
        "main": [
            {
                "adapter": "kotlin",
                "value": "com.nekkan.oldanimations.OldAnimations::init"
            }
        ]
    },
    "mixins": [
        "oldanimations.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.8.7",
        "fabric-language-kotlin": ">=1.4.21",
        "minecraft": "1.16.x"
    },
    "custom": {
        "modmenu:clientsideOnly": true
    }
}
